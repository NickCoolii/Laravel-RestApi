{
    "name": "malhal/laravel-restapi",
    "description": "A controller and handler that lets you easily build a REST API",
    "keywords": ["laravel", "framework", "rest", "api", "controller", "Laravel RestApi", "Laravel-RestApi", "Malcolm Hall", "malhal"],
    "license": "MIT",
    "authors": [
        {
            "name": "Malcolm Hall",
            "email": "malcolmhall@me.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Malhal\\RestApi\\": "src/"
        }
    },
    "autoload-dev": {
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
